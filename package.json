  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:backend": "cd backend && ./start.sh",
    "dev:frontend": "next dev",
    "lint": "eslint .",
    "start": "next start",
    "backend": "cd backend && source venv/bin/activate && python main.py",
    "backend:setup": "cd backend && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
